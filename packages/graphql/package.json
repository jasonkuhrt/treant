{
  "name": "@treant/graphql",
  "version": "0.0.1",
  "description": "Complete GraphQL parser with TypeScript SDK - wraps grammar WASM and SDK for easy use",
  "type": "module",
  "author": "Jason Kuhrt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonkuhrt/treant.git",
    "directory": "packages/graphql"
  },
  "keywords": [
    "tree-sitter",
    "graphql",
    "parser",
    "ast",
    "typescript",
    "wasm"
  ],
  "exports": {
    ".": {
      "import": "./build/$$.js"
    },
    "./grammar/wasm": "./grammar-build/grammar.wasm",
    "./grammar/grammar.json": "./grammar-build/grammar.json",
    "./grammar/node-types.json": "./grammar-build/node-types.json",
    "./grammar/queries/highlights.scm": "./grammar/queries/highlights.scm",
    "./grammar/queries/formatter.scm": "./grammar/queries/formatter.scm"
  },
  "scripts": {
    "generate": "treant generate all --out-dir-sdk src",
    "build": "pnpm generate && tsc -p tsconfig.build.json",
    "check:types": "tsc --noEmit"
  },
  "files": [
    "build",
    "grammar-build",
    "grammar"
  ],
  "dependencies": {
    "web-tree-sitter": "^0.25.0"
  },
  "devDependencies": {
    "@treant/cli": "workspace:*",
    "@treant/grammar": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
