{
  "name": "@treant/graphql",
  "version": "0.0.0",
  "description": "Complete GraphQL parser with TypeScript SDK - wraps grammar WASM and SDK for easy use",
  "type": "module",
  "author": "Jason Kuhrt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonkuhrt/treant.git",
    "directory": "packages/graphql"
  },
  "keywords": [
    "tree-sitter",
    "graphql",
    "parser",
    "ast",
    "typescript",
    "wasm"
  ],
  "exports": {
    ".": {
      "import": "./build/$.js"
    },
    "./wasm": "@treant/graphql-grammar/wasm"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest",
    "check:types": "tsc --noEmit"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@treant/graphql-grammar": "workspace:*",
    "@treant/graphql-sdk": "workspace:*",
    "web-tree-sitter": "^0.25.0"
  },
  "devDependencies": {
    "@treant/grammar": "workspace:*"
  }
}
