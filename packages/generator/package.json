{
  "name": "@treant/generator",
  "version": "0.0.0",
  "description": "TypeScript SDK generator for tree-sitter grammars",
  "type": "module",
  "author": "Jason Kuhrt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonkuhrt/treant.git",
    "directory": "packages/generator-sdk"
  },
  "exports": {
    ".": {
      "import": "./build/$.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check:types": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "generate:test:fixtures": "tsx tests/helpers/generate-sdk-fixtures"
  },
  "dependencies": {
    "@dprint/formatter": "^0.4.1",
    "@effect/platform": "^0.90.6",
    "@effect/platform-node": "^0.96.0",
    "@treant/core": "workspace:*",
    "@treant/grammar": "workspace:*",
    "effect": "^3.17.9"
  },
  "devDependencies": {
    "web-tree-sitter": "^0.25.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
