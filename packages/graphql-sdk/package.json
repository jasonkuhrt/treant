{
  "name": "@treant/graphql-sdk",
  "version": "0.0.0",
  "description": "TypeScript SDK for tree-sitter GraphQL parser",
  "type": "module",
  "author": "Jason Kuhrt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonkuhrt/treant.git",
    "directory": "packages/graphql-sdk"
  },
  "keywords": [
    "tree-sitter",
    "graphql",
    "parser",
    "typescript",
    "sdk",
    "ast"
  ],
  "exports": {
    ".": {
      "import": "./build/$.js"
    }
  },
  "scripts": {
    "generate": "treant generate library --grammar graphql",
    "build": "pnpm run generate && tsc -p tsconfig.build.json",
    "test": "vitest",
    "check:types": "tsc --noEmit"
  },
  "files": [
    "build"
  ],
  "peerDependencies": {
    "web-tree-sitter": "^0.25.0"
  },
  "devDependencies": {
    "web-tree-sitter": "^0.25.8",
    "@treant/cli": "workspace:*"
  }
}
