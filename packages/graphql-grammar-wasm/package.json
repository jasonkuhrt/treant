{
  "name": "@treant/graphql-grammar-wasm",
  "version": "1.2.0",
  "description": "Pre-built WASM binary for tree-sitter GraphQL grammar",
  "type": "module",
  "author": "Jason Kuhrt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonkuhrt/treant.git",
    "directory": "packages/graphql-grammar-wasm"
  },
  "keywords": [
    "tree-sitter",
    "graphql",
    "wasm",
    "parser",
    "binary"
  ],
  "exports": {
    ".": "./grammar.wasm"
  },
  "scripts": {
    "prebuild": "pnpm --filter @treant/graphql-grammar build",
    "build": "tree-sitter build --docker --wasm --output grammar.wasm ../graphql-grammar",
    "test": "vitest",
    "clean": "rm -f grammar.wasm"
  },
  "files": [
    "grammar.wasm",
    "README.md"
  ],
  "devDependencies": {
    "tree-sitter-cli": "^0.25.8"
  }
}
