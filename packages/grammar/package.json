{
  "name": "@treant/grammar",
  "version": "0.1.0",
  "description": "TypeScript types for tree-sitter grammar definitions",
  "type": "module",
  "author": "Jason Kuhrt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonkuhrt/treant.git",
    "directory": "packages/grammar"
  },
  "exports": {
    ".": {
      "import": "./build/$.js"
    },
    "./grammar": {
      "import": "./build/$$.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check:types": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "generate": "tsx scripts/generate-dsl/main"
  },
  "dependencies": {
    "@effect/platform": "^0.90.6",
    "@effect/platform-node": "^0.96.0",
    "@effect/schema": "^0.75.5",
    "effect": "^3.17.9",
    "tree-sitter-cli": "^0.25.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
