{
  "extends": ["@tsconfig/strictest", "@tsconfig/node24"],
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2022",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true
  }
}
